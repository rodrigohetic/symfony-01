{% extends 'base.html.twig' %}

{% block title %}New Quote
{% endblock %}

{% block body %}
    <div class="col-9">
        <h1>Create new Quote</h1>
        <div class="card mb-5">
            <div class="card-body">
                <h5 class="card-title">form</h5>
                {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
                    {{ form_widget(form) }}
                    {{ form_end(form) }}

                <a href="{{ path('quotes') }}">back to list</a>
            </div>
        </div>
    </div>
{% endblock %}